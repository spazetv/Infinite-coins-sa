local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Infinite Coins Generator", "Midnight")
local Tab = Window:NewTab("Inf coins")
local Section = Tab:NewSection("Inf Coins")
Section:NewButton("+1,000 coins", "Gets you +1,000 coins", function()
    for i=1,1 do
local args = {
    [1] = "winner"
}

workspace:WaitForChild("REs"):WaitForChild("easter"):FireServer(unpack(args))
end
end)
Section:NewButton("+10,000 coins", "Gets you +10,000 coins", function()
    for i=1,10 do
local args = {
    [1] = "winner"

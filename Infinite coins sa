Section:NewButton("Buy Easter x100", "Buys a case", function()
    for i=1,100 do 
	local args = {
    [1] = "boughtcase",
    [2] = "Easter"
}

workspace:WaitForChild("REs"):WaitForChild("shopInteract"):FireServer(unpack(args))
end
end)
